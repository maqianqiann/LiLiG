<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bj_bs"
    android:layout_marginTop="@dimen/x20"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <include
            layout="@layout/view_simple_topbar"
            ></include>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_weight="1"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="不感兴趣的"
                android:layout_marginTop="@dimen/x42"
                android:textSize="@dimen/x28"
                android:layout_marginRight="@dimen/x22"
                android:layout_marginLeft="@dimen/x500"
                android:id="@+id/txt_uninterest_keep_data"
                android:textColor="@color/colorNoInterestTwo"
                />
        </LinearLayout>


    </RelativeLayout>

<TextView
    android:layout_width="match_parent"
    android:layout_height="@dimen/x76"
    android:background="@color/colorKeep"
    android:text="100人"
    android:textColor="@color/colorWhite"
    android:gravity="center"
    />
  <ListView
      android:layout_margin="@dimen/x22"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:id="@+id/listView_keep_data"
      ></ListView>



</LinearLayout>